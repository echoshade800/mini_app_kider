{
  "name": "KidderCrushMiniApp",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "engines": {
    "node": ">=20.19.1"
  },
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 npx expo start",
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "clean": "rm -rf node_modules package-lock.json .expo",
    "doctor": "npx expo doctor",
    "build:ios": "npx react-native bundle --platform ios --dev false --entry-file index.tsx --bundle-output ios/rnbundle/main.jsbundle --assets-dest ios/rnbundle",
    "deploy:ios": "sh deploy_ios.sh"
  },
  "dependencies": {
    "@expo/cli": "^0.21.0",
    "@expo/metro-config": "~54.0.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "expo": "~54.0.0",
    "expo-av": "^16.0.7",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "metro": "^0.83.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@babel/core": "^7.26.0",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/metro-config": "^0.81.4",
    "@types/react": "~19.1.10",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "typescript": "~5.9.2"
  }
